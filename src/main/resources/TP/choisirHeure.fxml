<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="302.0" prefWidth="429.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TP.Controllers.choisirHeureController">
   <children>
      <VBox layoutX="46.0" layoutY="54.0" prefHeight="106.0" prefWidth="278.0">
         <children>
            <HBox prefHeight="62.0" prefWidth="278.0" spacing="15.0">
               <children>
                  <Label text="Start" />
                  <Spinner fx:id="hour1" editable="true" prefHeight="25.0" prefWidth="90.0" />
                  <Spinner fx:id="minute1" editable="true" prefHeight="25.0" prefWidth="90.0" />
               </children>
            </HBox>
            <HBox prefHeight="47.0" prefWidth="278.0" spacing="15.0">
               <children>
                  <Label contentDisplay="CENTER" text="End  " />
                  <Spinner fx:id="hour2" editable="true" prefHeight="25.0" prefWidth="90.0" />
                  <Spinner fx:id="minute2" editable="true" prefHeight="25.0" prefWidth="90.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="AddCreneau" layoutX="250.0" layoutY="227.0" mnemonicParsing="false" onAction="#addNewSlot" prefHeight="25.0" prefWidth="90.0" style="-fx-background-color: #F8D1D0; -fx-background-radius: 15; -fx-border-radius: 15;" text="Add">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="cancelCreneau" layoutX="69.0" layoutY="227.0" mnemonicParsing="false" onAction="#cancelCreneau" prefHeight="25.0" prefWidth="90.0" style="-fx-background-color: #F8D1D0; -fx-background-radius: 15; -fx-border-radius: 15;" text="Cancel">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
